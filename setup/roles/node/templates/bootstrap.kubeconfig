apiVersion: v1
clusters:
- cluster:
    certificate-authority: "{{ cert_dir }}/ca.pem"
    server: https://127.0.0.1:9443
  name: kubernetes
contexts:
- context:
    cluster: kubernetes
    user: kubelet-bootstrap
  name: default
current-context: default
kind: Config
preferences: {}
users:
- name: kubelet-bootstrap
  user:
    token: {{ BOOTSTRAP_TOKEN }} 
